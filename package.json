{
  "name": "json-edit-vue3",
  "version": "1.0.7",
  "private": false,
  "description": "基于vue3的json编辑器",
  "type": "module",
  "main": "dist/json-edit.umd.js",
  "module": "dist/json-edit.es.js",
  "exports": {
    "./dist/style.css": "./dist/style.css",
    ".": {
      "import": "./dist/json-edit.es.js",
      "require": "./dist/json-edit.umd.js"
    }
  },
  "scripts": {
    "pre": "yarn",
    "dev": "vite --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false"
  },
  "files": [
    "dist/*",
    "*.json",
    "*.md",
    "*.lock"
  ],
  "license": "MIT",
  "author": "niezihao",
  "dependencies": {
    "less": "^4.2.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/tsconfig": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.8"
  }
}